{
  "name": "Wisewe",
  "private": false,
  "version": "0.0.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "commit": "git-cz",
    "build": "yarn build:components && build:umd && build:esm &&yarn build:tools",
    "build:components": "npm run build:esm && npm run build:umd",
    "build:cjs": "rollup --config ./build/rollup.config.cjs.js",
    "build:umd": "rollup --config ./build/rollup.config.umd.js",
    "build:esm": "rollup --config ./build/rollup.config.esm.js",
    "build:tools": "cross-env BABEL_ENV=utils babel packages/tools --extensions .ts --out-dir lib/tools",
    "build:theme": "gulp build --gulpfile packages/theme-chalk/gulpfile.js",
    "upgrade:v": "bash ./scripts/update-versions.sh",
    "fast-build": "sh ./scripts/build.sh",
    "test": "jest"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-lerna-changelog"
    }
  },
  "main": "lib/index.umd.js",
  "module": "lib/index.esm.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/wangg-912/wisewe-repo.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push-todo-open": "npm run test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix"
  },
  "files": [
    "lib",
    "es",
    "packages"
  ],
  "peerDependencies": {
    "vue": "3.1.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-typescript": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-eslint": "^4.5.13",
    "@vue/cli-plugin-typescript": "^4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/compiler-sfc": "^3.1.4",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-vue": "^2.0.2",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.6",
    "cz-lerna-changelog": "^2.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^7.0.0-0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-serializer-vue": "^2.0.2",
    "lerna": "^4.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^1.19.1",
    "rollup": "^2.28.2",
    "rollup-plugin-css-only": "^2.1.0",
    "rollup-plugin-esbuild": "^4.2.3",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.3",
    "rollup-plugin-vue": "^6.0.0",
    "rollup-pluginutils": "^2.8.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "standard": "^16.0.3",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "vue-jest": "^3.0.7",
    "vue-loader": "^16.3.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.161",
    "element-plus": "^1.0.2-beta.55",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^8.0.0",
    "gulp-cssmin": "^0.2.0",
    "gulp-dart-sass": "^1.0.2",
    "gulp-rename": "^2.0.0",
    "lodash": "^4.17.20",
    "tslib": "^2.3.0",
    "vue": "^3.1.1",
    "vue-template-compiler": "^2.6.14"
  },
  "license": "MIT"
}
